<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="inscricoesRoot"
            xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            styleClass="content-area">

    <children>
        <VBox spacing="24.0"
              AnchorPane.topAnchor="40.0"
              AnchorPane.leftAnchor="40.0"
              AnchorPane.rightAnchor="40.0"
              AnchorPane.bottomAnchor="40.0">

            <VBox fx:id="inscricaoCard1"
                  styleClass="card">
                <Label text="Nome da palestra" styleClass="card-title"/>
                <Label text="palestrante:" styleClass="card-text"/>
                <Label text="sala:" styleClass="card-text"/>
                <Label text="data/Hora:" styleClass="card-text"/>
                <Label text="duração em horas:" styleClass="card-text"/>
            </VBox>

            <VBox fx:id="inscricaoCard2"
                  styleClass="card">
                <Label text="Nome da palestra 2" styleClass="card-title"/>
                <Label text="palestrante:" styleClass="card-text"/>
                <Label text="sala:" styleClass="card-text"/>
                <Label text="data/Hora:" styleClass="card-text"/>
                <Label text="duração em horas:" styleClass="card-text"/>
            </VBox>
        </VBox>
    </children>
</AnchorPane>
